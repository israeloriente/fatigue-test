{
  "name": "fatigue-test",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "socat": "socat -d -d pty,raw,echo=0 pty,raw,echo=0",
    "electron:start": "vite build && electron .",
    "electron:dev": "concurrently \"vite\" \"npm run electron:start\"",
    "electron:build": "npm run build && electron-builder"
  },
  "build": {
    "appId": "io.israeloriente.fatigue-test",
    "productName": "Fatigue Test",
    "files": [
      "**/*"
    ],
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "target": "nsis"
    }
  },
  "dependencies": {
    "bootstrap": "^5.3.3",
    "chart.js": "^4.4.7",
    "net": "^1.0.2",
    "node-arp": "^1.0.6",
    "pinia": "^2.3.0",
    "serialport": "^13.0.0",
    "three": "^0.172.0",
    "vue": "^3.5.13",
    "vue-i18n": "^11.0.1",
    "vue-router": "^4.5.0",
    "vue-sweetalert2": "^5.0.11"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "concurrently": "^9.1.2",
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8",
    "sass-embedded": "^1.83.1",
    "typescript": "~5.6.2",
    "vite": "^6.0.5",
    "vite-plugin-static-copy": "^2.2.0",
    "vue-tsc": "^2.2.0"
  }
}
